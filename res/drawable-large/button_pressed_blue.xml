<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval" >
            <solid android:color="#375D81" />

            <stroke
               
                android:width="12sp"
                android:color="#fff" />
        </shape>
    </item>
    <item
        android:bottom="25dp"
        android:left="25dp"
        android:right="25dp"
        android:top="25dp">
        <shape android:shape="oval" >
            <solid android:color="#ffffff" />

            <stroke
                android:width="8sp"
                android:color="#375D81" />
        </shape>
    </item>


</layer-list>

